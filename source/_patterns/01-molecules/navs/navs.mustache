{{#navs}}
    <ul class="m-navs nav {{ styleModifier }} {{ class }}">
        {{# items }}
            <li class="nav-item">
                <a class="nav-link {{# isActive }}active{{/ isActive }} {{# isDisabled }}disabled{{/ isDisabled }}"
                   href="{{ url }}">
                    {{{ text }}}

                    {{# subnav}}
                        <i class="fa fa-angle-down"></i>
                    {{/subnav}}
                </a>
            </li>
        {{/ items }}
    </ul>
{{/navs}}